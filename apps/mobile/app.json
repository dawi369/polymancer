{
  "expo": {
    "name": "polymancer",
    "slug": "polymancer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ios-master.png",
    "scheme": "polymancer",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "icon": {
        "light": "./assets/ios-master.png",
        "dark": "./assets/android-foreground.png",
        "tinted": "./assets/android-foreground.png"
      },
      "bundleIdentifier": "ai.polymancer",
      // might be able to get rid of this later
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-foreground.png",
        "backgroundColor": "#000000"
      },
      "predictiveBackGestureEnabled": false,
      "package": "ai.polymancer"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/ios-master.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/android-foreground.png",
          "imageWidth": 250
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5a55fc92-39c1-4761-bbf3-1205552fbe9f"
      }
    },
    "owner": "dawi369"
  }
}
